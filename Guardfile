# A sample Guardfile
# More info at https://github.com/guard/guard#readme

guard 'sass', :style => :expanded, :input => 'UniversityCollege_cssCleanup/scss', :output => 'UniversityCollege_cssCleanup/css'
guard 'sass', :style => :compressed, :input => 'UniversityCollege_cssCleanup/scss', :output => 'UniversityCollege_cssCleanup'
